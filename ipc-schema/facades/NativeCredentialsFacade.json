{
	"name": "NativeCredentialsFacade",
	"type": "facade",
	"senders": ["web"],
	"receivers": ["ios", "desktop", "android"],
	"doc": "Operations for credential encryption operations using OS keychain.",
	"methods": {
		"encryptUsingKeychain": {
			"arg": [{ "data": "bytes" }, { "encryptionMode": "CredentialEncryptionMode" }],
			"ret": "bytes"
		},
		"decryptUsingKeychain": {
			"arg": [{ "encryptedData": "bytes" }, { "encryptionMode": "CredentialEncryptionMode" }],
			"ret": "bytes"
		},
		"getSupportedEncryptionModes": {
			"arg": [],
			"ret": "List<CredentialEncryptionMode>"
		},
		"loadAll": {
			"arg": [],
			"ret": "List<PersistedCredentials>"
		},
		"store": {
			"arg": [{ "credentials": "PersistedCredentials" }],
			"ret": "void"
		},
		"loadByUserId": {
			"arg": [{ "id": "string" }],
			"ret": "PersistedCredentials?"
		},
		"deleteByUserId": {
			"arg": [{ "id": "string" }],
			"ret": "void"
		},
		"getCredentialEncryptionMode": {
			"arg": [],
			"ret": "CredentialEncryptionMode?"
		},
		"setCredentialEncryptionMode": {
			"arg": [{ "encryptionMode": "CredentialEncryptionMode" }],
			"ret": "void"
		},
		"getCredentialsEncryptionKey": {
			"arg": [],
			"ret": "bytes"
		},
		"setCredentialsEncryptionKey": {
			"arg": [{ "credentialsEncryptionKey": "bytes?" }],
			"ret": "void"
		}
	}
}
